	function fireatwill()
		Attack = true	
		Humanoid.WalkSpeed = 8
		Humanoid.AutoRotate = false
		local bodygyro = Instance.new("BodyGyro",RootPart)
		bodygyro.MaxTorque = Vector3.new(50000,50000,50000)
		bodygyro.P = 8000	

		for i = 0, 6, 1 do
			if AmmoCount <= 0 then 
				break
			end	
			Torso.Velocity=RootPart.CFrame.lookVector*-5
			for i = 0, .5, 0.1 do
				swait()
