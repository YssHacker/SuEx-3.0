		end)
	end)



	------ Scripts end ----

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		rainbow1.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
		rainbow2.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end
end)
okjp.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = textjp.Text
end)
WsOk.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = WalkSpeedText.Text
end)
tpok.MouseButton1Down:connect(function()
	local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
	local pl2 = TpPlyer.Text
	local humanoid = game.Players.LocalPlayer.Character.Humanoid
	humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	wait(0.1)

	pl.CFrame = game.Players[pl2].Character.HumanoidRootPart.CFrame
end)
