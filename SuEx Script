	function makeframe(par, trans, pos, size, color)
		local frame = Instance.new('Frame', par)
		frame.BackgroundTransparency = trans
		frame.BorderSizePixel = 0
		frame.Position = pos
		frame.Size = size
		frame.BackgroundColor3 = color
		return frame
	end


