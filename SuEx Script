				CFrame.new(-0.367935598, 0.648825228, -1.07648134, 0.387547314, -0.915556669, -0.107533015, -0.209657595, 0.0260535926, -0.977427781, 0.897691965, 0.401344508, -0.181856394) * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, 0), 
