	MainLogo.ImageTransparency = 1.000
	MainLogo.ScaleType = Enum.ScaleType.Fit
	MainLogo.SliceCenter = Rect.new(100, 100, 100, 100)
	MainLogo.SliceScale = 0.120

	MainLogo2.Name = "MainLogo2"
	MainLogo2.Parent = MainLogo
