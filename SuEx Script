		Humanoid.WalkSpeed = 5
		Humanoid.JumpPower = 0
		CFuncs.Sound.Create("1073355392", Stockbox, 1, 1)
		for i = 0, 1.5, 0.1 do
			swait()
			PlayAnimationFromTable({
