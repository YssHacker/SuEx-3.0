						smonk.Transparency = bbb
						smonk.Size = aaa
						smonk.ZOffset = 0
						smonk.Acceleration = Vector3.new(0, -5, 0)
						smonk.LockedToPart = false
						smonk.EmissionDirection = "Back"
						smonk.Lifetime = NumberRange.new(.1, .5)
						smonk.Rate = 50
						smonk.Rotation = NumberRange.new(-100, 100)
